{
  "name": "cadasta-arcgis-admin-api",
  "version": "1.0.0",
  "description": "",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "start": "webpack -w",
    "test": "jest",
    "build": "NODE_ENV=${NODE_ENV:-production} webpack",
    "package": "sam package --template-file template.yaml --s3-bucket $npm_package_name-${STAGE:-dev} --output-template-file template.packaged.yaml",
    "deploy": "sam deploy --template-file template.packaged.yaml --stack-name $npm_package_name-${STAGE:-dev} --capabilities CAPABILITY_IAM --region us-west-2 --parameter-overrides Stage=${STAGE:-dev}",
    "clean": "rm -rf -v dist/*",
    "validate-template": "aws cloudformation validate-template --template-body file://template.yaml"
  },
  "repository": {},
  "author": "",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@esri/arcgis-rest-groups": "^1.4.1",
    "@esri/arcgis-rest-request": "^1.4.1",
    "@types/isomorphic-fetch": "0.0.34",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-form-data": "^1.0.0",
    "slugify": "^1.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.3",
    "@types/aws-sdk": "^2.7.0",
    "@types/chance": "^1.0.1",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.0.4",
    "@types/rosie": "0.0.35",
    "@types/uuid": "^3.4.3",
    "aws-sdk": "^2.234.1",
    "chance": "^1.0.16",
    "jest": "^23.1.0",
    "rosie": "^2.0.1",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.3",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.3",
    "yaml-cfn": "^0.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
